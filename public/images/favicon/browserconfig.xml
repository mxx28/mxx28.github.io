<?xml version="1.0" encoding="utf-8"?>
<!-- Windows tile configuration: point to generated ms-icon images under /images/favicon/ -->
<browserconfig>
	<msapplication>
		<tile>
			<square70x70logo src="/images/favicon/ms-icon-70x70.png"/>
			<square150x150logo src="/images/favicon/ms-icon-150x150.png"/>
			<square310x310logo src="/images/favicon/ms-icon-310x310.png"/>
			<TileColor>#2e59ba</TileColor>
		</tile>
	</msapplication>
</browserconfig>